// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

void intersectionNum(int arr1[], int arr2[], int n, int m) {
    
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < m; j++) {
            if(arr1[i] == arr2[j]) {
                cout << arr1[i] << " ";
            }
        }
    }
    
    
}

int main() {
    int arr1[] = {1, 2, 3, 4, 5};
    int arr2[] = {6, 7, 3, 1};
    int size1 = sizeof(arr1) / sizeof(int);
    int size2 = sizeof(arr2) / sizeof(int);
    
    intersectionNum(arr1, arr2, size1, size2);

    return 0;
}
