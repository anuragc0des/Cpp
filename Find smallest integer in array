#include <iostream>
#include <climits>
using namespace std;

int main() {
    int arr[] = {132, 21, 36, 42, 5};
    int size = sizeof(arr) / sizeof(int);
    int smallest = INT_MAX;
    
    for(int i = 0; i < size; i++) {
        if(arr[i] < smallest) {
            smallest = arr[i];
        }
    }
    
    cout << smallest;
    
    return 0;
}
