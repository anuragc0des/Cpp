// CHECKING IF A NUMBER IS PRIME OR NOT
#include <iostream>
using namespace std;

int main() {
    int n = 12;
    bool isPrime = true; // initializing isPrime as true
    
    for(int i = 2; i * i <= n; i++) { // we have to use i * i <= n, this checks upto sqrt(n)
        if(n % i == 0) {
            isPrime = false; // if is remainder 0 -> isPrime will become false
            break; // and we'll come out of the loop
        } 
    }

    // just a condition to print prime string
    if(isPrime) {
        cout << "Prime";
    } else {
        cout << "Not Prime";
    }
    
    
    return 0;
}



// PRINTING PRIME NUMBER FROM 1 TO N

#include <iostream>
using namespace std;

int main() {
    
    int n = 10;
    
    for(int i = 1; i <= n; i++) {
        bool isPrime = true;
        
        if(i <= 1) {
            isPrime = false;
        } else {
            for(int j = 2; j * j <= i; j++) { 
                if(i % j == 0) {
                    isPrime = false;
                    break;
                }
            }
        }
        
        
        
        if(isPrime) {
            cout << i << endl;
        }
    }
    
    return 0;
}
