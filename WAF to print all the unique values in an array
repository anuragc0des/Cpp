// Online C++ compiler to run C++ program online
#include <iostream>
#include <climits>

using namespace std;

void uniqueValue(int arr[], int n) {
    
    
    for(int i = 0; i < n; i++) {
        bool isUnique = true;

        for(int j = 0; j < n; j++) {
            if(i != j && arr[i] == arr[j]) {
                isUnique = false;
                break;
            } 
        }
        
        if(isUnique) {
            cout << arr[i];
            return;
        }
    }
    
    
    
    // cout << num;
    
};

int main() {
    // Write C++ code here
    int arr[] = {1,2,3,4,1,9,2,3,4};
    int size = sizeof(arr) / sizeof(int);
    
    uniqueValue(arr, size);

    return 0;
}
