
#include <iostream>
using namespace std;

void fibonacci(int n) {
    int sum = 0;
    int x = n;
    int a = 0;
    int b = 1;
    
    for(int i = 0; i < x; i++) {
        cout << a << " ";
        sum = a + b;
        a = b;
        b = sum;
    }
}

int main() {
    fibonacci(7);
    return 0;
}
